{
  "compilerOptions": {
    "module": "commonjs",
    "target": "es6",
    "lib": ["es6"],
    "moduleResolution": "node",
    "sourceMap": false,
    "declaration": true,
    // "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "experimentalDecorators": true,
    "outDir": "./generators"
  },
  "include": ["src/**/*.ts"],
  "exclude": ["node_modules", "src/templates", "generators"]
}
