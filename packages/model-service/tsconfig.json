{
  "compilerOptions": {
    "target": "ES2017",
    "module": "Node16",
    "lib": ["ESNext"],
    "sourceMap": true,
    "outDir": "out",
    "strict": true,
    "noUnusedLocals": true,
    "noImplicitAny": false,
    "noImplicitReturns": true,
    "noImplicitOverride": true,
    "moduleResolution": "Node16",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "declaration": true
  },
  "include": ["src"]
}
